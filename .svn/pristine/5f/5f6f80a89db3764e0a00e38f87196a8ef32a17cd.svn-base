<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.dorado.generated.persistence.dmchannelmaster.integration.LINCSTaxType"
        table="T_LINCSTAXTYPE"
        lazy="true">

        <meta attribute="class-description" inherit="false">
              @hibernate.class
              table="T_LINCSTAXTYPE"
        </meta>


        <id
            name="typeCode"
            column="typeCode"
            type="java.lang.String"
            >
            <meta attribute="field-description">
                @hibernate.id
                generator-class="native"
                type="java.lang.String"
                column="typeCode"
            </meta>
            <meta attribute="use-in-equals">true</meta>
            <generator class="native">
            </generator>
        </id>


        <property
            name="description"
            type="java.lang.String"
            column="description"
            length="255"
            not-null="false"
            unique="false"
            >
            <meta attribute="field-description">
                @hibernate.property
                column="description"
                length="255"
            </meta>
        </property>

        <property
            name="segment"
            type="java.lang.String"
            column="segment"
            length="10"
            not-null="false"
            unique="false"
            >
            <meta attribute="field-description">
                @hibernate.property
                column="segment"
                length="10"
            </meta>
        </property>

        <property
            name="disbursmentCode"
            type="java.lang.String"
            column="disbursmentCode"
            length="10"
            not-null="false"
            unique="false"
            >
            <meta attribute="field-description">
                @hibernate.property
                column="disbursmentCode"
                length="10"
            </meta>
        </property>


    </class>

</hibernate-mapping>

