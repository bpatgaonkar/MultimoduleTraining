				for FHA Loan Transmittal.
				@hibernate.many-to-one not-null="true" cascade="none"
				@hibernate.column name="subordinateFinanceSourceType"
			</meta>
		</many-to-one>

		<!-- bi-directional one-to-many association to DownPayment -->
		<set name="DownPayments" table="T_DownPayment" lazy="true"
			inverse="true" cascade="all" order-by="1">
			<meta attribute="default-value"><![CDATA[ new java.util.LinkedHashSet<DownPayment>(0)]]></meta>
			<meta attribute="field-description">
				@hibernate.set lazy="true" inverse="true" cascade="all"
				@hibernate.collection-key column="transactionId"
				@hibernate.collection-one-to-many
				class="com.dorado.generated.persistence.dmchannelmaster.transaction.DownPayment"
				/>
			</meta>
			<meta attribute="do-not-generate-transferobject-collection"/>
			<key column="transactionId" />
			<one-to-many
				class="com.dorado.generated.persistence.dmchannelmaster.transaction.DownPayment" />
		</set>

		<!-- bi-directional one-to-one association to LoanApplication -->
		<one-to-one name="loanApplication"
			class="com.dorado.generated.persistence.model.loan.LoanApplication"
			property-ref="transaction">
			<meta attribute="field-description">
				name="loanApplication"
				class="com.dorado.generated.persistence.model.loan.LoanApplication"
				property-ref="transaction" />
			</meta>
			<meta attribute="do-not-generate-transferobject-property" />
		</one-to-one>

		<!-- bi-directional one-to-many association to OtherCredit -->
		<set name="OtherCreditsInternal" table="T_OtherCredit"
			lazy="true" inverse="true" cascade="all-delete-orphan" order-by="1">
			<meta attribute="default-value"><![CDATA[ new java.util.LinkedHashSet<OtherCredit>(0)]]></meta>
			<meta attribute="field-description">
				@hibernate.set lazy="true" inverse="true" "all-delete-orphan"
				@hibernate.collection-key column="transactionId"
				@hibernate.collection-one-to-many
				class="com.dorado.generated.persistence.dmchannelmaster.transaction.OtherCredit"
				/>
			</meta>
			<meta attribute="scope-set">protected</meta>
			<meta attribute="scope-get">protected</meta>
			<key column="transactionId" />
			<one-to-many
				class="com.dorado.generated.persistence.dmchannelmaster.transaction.OtherCredit" />
		</set>

