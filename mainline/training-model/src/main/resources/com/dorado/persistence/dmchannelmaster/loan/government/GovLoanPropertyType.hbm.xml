<?xml version="1.0"?> 
 <!DOCTYPE hibernate-mapping 
  PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" 
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping> 

	<class
		name="com.dorado.generated.persistence.dmchannelmaster.loan.government.GovLoanPropertyType"
		table="T_GOVLOANPROPERTYTYPE" lazy="true">

		<meta attribute="class-description" inherit="false">
			Special property type for FHA Loan Transmittal.  Maps number of units and
			Property Type to one (GovLoan) Property Type.
			@hibernate.class
			table="T_GOVLOANPROPERTYTYPE"
		</meta>
	    <meta attribute="implement-equals">true</meta>
	    <meta attribute="implements">com.dorado.persistence.EnumeratedType</meta>
	    <meta attribute="implements">com.dorado.persistence.Described</meta>

		<id 
			name="typeCode"  
			column="GovLoanPropertyTypeCode"  
			type="java.lang.String" 
			> 
			<meta attribute="field-description">
					@hibernate.id
					generator-class="native"
					type="java.lang.String" 
					column="GovLoanPropertyTypeCode"  
				</meta>
				<meta attribute="use-in-equals">true</meta>
				<generator class="native">
			</generator> 
		</id> 

		<property 
			name="displayOrder" 
			type="java.lang.Integer" 
			column="displayOrder" 
			length="22" 
			not-null="false" 
			unique="false"  
			> 
			<meta attribute="field-description"> 
				@hibernate.property 
				column="displayOrder" 
				length="22" 
			</meta> 
		</property> 
		
		<property 
			name="description" 
			type="java.lang.String" 
			column="description" 
			length="256" 
			not-null="false" 
			unique="false"  
			> 
			<meta attribute="field-description"> 
				@hibernate.property 
				column="description" 
				length="256" 
			</meta> 
		</property> 
	</class> 
 </hibernate-mapping> 